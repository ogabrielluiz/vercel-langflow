{
    "devCommand": "uvicorn api.main:app --host 0.0.0.0 --port 7860",
    "builds": [
        {
            "src": "api/main.py",
            "use": "@vercel/python"
        }
    ],
    "routes": [
        {
            "src": "/(.*)",
            "dest": "api/main.py"
        }
    ],
    "installCommand": "yum groupinstall \"Development Tools\""
}